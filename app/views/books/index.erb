<h1>Your Collection of Books:</h1>
<br>
<% @books.each do |book| %>
  <ol>
  	<li>
  	  <a href="/books/<%= book.slug %>"><%= book.title %>"</a>, 
  	  written by <%= book.author.name %>, 
  	  <% book.genres.each do |genre| %>
  	  	<%= genre.name %>
  	  <% end %>
  	  <br><br>
  	</li>
  </ol>
<% end %>  
<br>
<br>
<br>
<a href="/logout">Log Out</a>
